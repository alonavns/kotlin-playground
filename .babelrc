{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "useBuiltIns": "usage",
        "corejs": 2,
        "exclude": ["transform-async-to-generator", "transform-regenerator"]
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "helpers": true,
        "corejs": 2,
        "regenerator": false
      }
    ],
    "module:fast-async"
  ]
}
